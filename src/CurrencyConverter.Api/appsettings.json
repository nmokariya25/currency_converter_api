{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Frankfurter": {
    "BaseUrl": "https://api.frankfurter.app",
    "TimeoutSeconds": 10
  },
  "Jwt": {
    "Key": "ThisIsAStrongSecretKeyWithMoreThan32Bytes",
    "Issuer": "YourIssuer",
    "Audience": "YourAudience"
  },
  "RateLimiting": {
    "PermitLimit": 100,
    "WindowSeconds": 60
  },
  "Caching": {
    "DefaultTTLSeconds": 300
  }
}
